program calculoimc;

var
    peso: integer;
    altura, imc: real;
    
begin

    read(peso, altura);
    imc:= peso/(altura*altura);
    
    if (altura = 0) then
        writeln('ERRO');
    
    writeln(imc:2:2);
    
        if (imc > 18.5) and (imc < 25) then
            writeln('SIM')
        else
            writeln('NAO');

end.
